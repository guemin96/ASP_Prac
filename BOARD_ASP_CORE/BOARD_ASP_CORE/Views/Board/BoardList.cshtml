@using System.Data
@{
    var dtList = Model.Tables[0];

}

<head>

</head>
<body>
    <div>
        <p>글쓰기</p>
    </div>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>제목</th>
                <th>조회수</th>
                <th>작성일</th>
            </tr>
        </thead>
        <tbody>
            @foreach (DataRow item in dtList.Rows) {
                <tr>
                    <td>@item["Id"]</td>
                    <td>@item["Title"]</td>
                    <td>@item["ReadCount"]</td>
                    <td>@item["PostDate"]</td>
                </tr>
            }
        </tbody>
    </table>
</body>