<form asp-controller="Home" asp-action="Register" method="post">
    <div class="form-group">
        <label>ID : </label>
        <input type="text" placeholder="사용자 ID입력" name="UserId"  />
    </div>

    <div class="form-group">
        <label>비밀번호 : </label>
        <input type="password" placeholder="사용자 비밀번호 입력" name ="UserPassword"   />
    </div>
    <div>
       <button type="submit" class="btn btn-primary">회원가입</button>
       <a class="btn btn-warning" href="#">취소</a>
    </div>
</form>